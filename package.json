{
  "name": "meike-build",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --config webpack.config.js --watch --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "files": [
    "bin/*",
    "dist/*.js",
    "@types/*.d.ts"
  ],
  "keywords": [
    "build",
    "meike",
    "vue",
    "vue.js",
    "vuejs",
    "vue ssr",
    "vue server side",
    "ssr"
  ],
  "bin": {
    "service:start": "./bin/start",
    "service:build": "./bin/build",
    "service:dev": "./bin/dev"
  },
  "typings": "@types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imsunhao/build.git"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.16.0",
    "@types/friendly-errors-webpack-plugin": "^0.1.1",
    "@types/memory-fs": "^0.3.2",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.3",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "babel-loader": "^8.0.4",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "consola": "^2.2.4",
    "esm": "^3.0.84",
    "minimist": "^1.2.0",
    "vue-server-renderer": "^2.5.17"
  }
