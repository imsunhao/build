{
  "name": "@bestminr/build-doc",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "node --max-old-space-size=6144 ./node_modules/@bestminr/build/bin/dev --ic ./inject-context/stage.json",
    "dev:release": "yarn dev --ic ./inject-context/release.json",
    "dev:dll": "yarn dev -d",
    "dev:noCache": "yarn dev:dll && yarn dev:release --cl",
    "build": "node ./node_modules/@bestminr/build/bin/build",
    "build:dll": "yarn build -d",
    "start": "node ./node_modules/@bestminr/build/bin/start",
    "start:stage": "cross-env SERVER_ENV=local yarn start --ic ./inject-context/stage.json",
    "start:release": "cross-env SERVER_ENV=local yarn start --ic ./inject-context/release.json",
    "debug:dev": "node --inspect-brk=32000 ./node_modules/@bestminr/build/bin/dev --ic ./inject-context/release.json",
    "debug:start": "node --inspect-brk=32000 ./node_modules/@bestminr/build/bin/start --ic ./inject-context/release.json",
    "debug:build": "node --inspect-brk=32000 ./node_modules/@bestminr/build/bin/build",
    "tslint": "tslint --project tsconfig.json -c tslint.json src/**/*.ts",
    "test": "jest tests --coverage",
    "test:watch": "cross-env NODE_ENV=test jest tests --watch"
  },
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "moduleRoots": [
    ".",
    "src"
  ],
  "dependencies": {
    "@bestminr/build": "1.6.18",
    "axios": "^0.17.0",
    "cross-env": "^4.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "vue": "^2.6.10",
    "vue-class-component": "^6.3.2",
    "vue-loader": "^15.4.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/runtime": "^7.1.5",
    "@types/babel-core": "^6.25.5",
    "@types/bluebird": "3.5.18",
    "@types/express": "^4.16.0",
    "@types/http-proxy-middleware": "^0.17.6",
    "@types/jest": "^23.3.13",
    "@types/minimist": "^1.2.0",
    "@types/webpack": "^4.4.19",
    "@types/webpack-env": "^1.13.6",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^4.10.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-vue": "^4.2.0",
    "file-loader": "^1.1.10",
    "happypack": "^5.0.0-beta.3",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "stylelint": "^9.10.0",
    "stylelint-scss": "^3.5.1",
    "ts-jest": "^24.0.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "vue-jest": "^3.0.4",
    "worker-loader": "^2.0.0"
  }
}
